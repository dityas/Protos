{
    "configuration": {
        "status": "error",
        "name": "libADDLogger",
        "packages": "libADD",
        "ThresholdFilter": {
            "level": "debug"
        },
        "appenders": {
            "Console": {
                "name": "STDOUT",
                "PatternLayout": {
                    "pattern": "%d %-5p %c{2} - %m%n"
                }
            }
        },
        "loggers": {
            "asyncroot": {
                "level": "debug",
                "AppenderRef": {
                    "ref": "STDOUT"
                }
            }
        }
    }
}
